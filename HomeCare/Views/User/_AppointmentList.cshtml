@model List<HomeCare.Models.Appointment>



@if (Model != null && Model.Any())
{
    <table class="table table-booking-custom">
        <thead>
            <tr>
                <th>Dato & Tidspunkt</th>
                <th>Kategorier</th>
                <th>Notater</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var a in Model)
            {
                <tr>
                    <td>@a.DateTime.ToString("yyyy-MM-dd HH:mm")</td>
                    <td>@a.Category?.Name</td>
                    <td>@a.Notes</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
